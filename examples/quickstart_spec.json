{
  "table": { "bubble": false, "level": 10 },
  "variables": { "units": 10 },
  "modes": { "Main": { "template": { "pass": "units", "place": { "6": "units*2", "8": "units*2" } } } },
  "rules": [],
  "run": {
    "csv": {
      "enabled": true,
      "path": ".artifacts/journal.csv",
      "append": true
    },
    "memory": {
      "meta_path": ".artifacts/meta.json",
      "report_path": ".artifacts/report.json"
    },
    "report": {
      "path": ".artifacts/report.json",
      "auto": false,
      "export": { "path": ".artifacts/exports", "compress": false }
    }
  }
}